{
  "name": "@comandeer/babel-preset-rollup",
  "version": "6.0.0",
  "description": "Evergreen Babel preset for rollup",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "lint": "eslint index.js test.js",
    "pretest": "npm run lint",
    "test": "mocha test.js --timeout 15000",
    "preversion": "npm test",
    "postversion": "git push origin && git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Comandeer/babel-preset-rollup.git"
  },
  "keywords": [
    "babel-preset",
    "rollup"
  ],
  "author": "Comandeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Comandeer/babel-preset-rollup/issues"
  },
  "homepage": "https://github.com/Comandeer/babel-preset-rollup#readme",
  "commitplease": {
    "nohook": true,
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "build",
      "ci",
      "chore",
      "revert"
    ],
    "scope": "\\S+.*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@comandeer/eslint-config": "^0.2.0",
    "@comandeer/is-ci": "^2.0.0",
    "chai": "^4.2.0",
    "commitplease": "^3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "in-publish": "^2.0.0",
    "mocha": "^6.1.4"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5"
  }
}
